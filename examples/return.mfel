function f(x) {
    if x > 20 {
        @println("return at beginning");     
        return 20;
    }
    let i = 0;
    while i < 15 {
        if i >= x {
            @println("return in while");     
            return i;
        }
        i = i + 1;   
    }
    @println("return at end");     
    return i;
}

@test("f(9)",9);
@test("f(17)",15);
@test("f(25)",20);
