let test := module () {
    let x := 10;
    let y := 20;
    let f := closure() { x + y };
    let m := module (x,y) {
        # shadows x and y (by value)
        let f := closure() { x + y };
    }
};


@println(test.f());
@println(test.m.f());

@set(test,"x",110);

@println(test.f());
@println(test.m.f());

@set(test.m,"x",210);

@println(test.f());
@println(test.m.f());

